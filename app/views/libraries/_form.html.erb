<%= simple_form_for(@library) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name, placeholder: 'ex. My Library', input_html: { class: 'textinput' },label_html: { class: 'label-text' } %>
  <%= f.input :address, :input_html => { :id => 'geo-address', class: 'textinput' }, placeholder: 'ex. 123 Sullivan st.',label_html: { class: 'label-text' } %>

  <%= f.button :submit, class: "sign-up btn-outline" %>
<% end %>
